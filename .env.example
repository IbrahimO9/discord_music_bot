# Discord Bot Token (Get from Discord Developer Portal -> Bot tab)
DISCORD_TOKEN=your_bot_token_here

# Discord Client ID (Get from Discord Developer Portal -> General Information tab)
CLIENT_ID=your_client_id_here

# Optional: Override Piped instance for streaming (defaults to https://piped.video)
PIPED_INSTANCE=

# Optional: URL Render should ping to prevent sleeping (set to your service URL)
KEEPALIVE_URL=